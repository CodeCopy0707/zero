# Agent Zero Gemini Configuration

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=8192
GEMINI_SAFETY_SETTINGS=default

# Agent Configuration
AGENT_NAME=Agent Zero Gemini
AGENT_DESCRIPTION=A powerful AI agent powered by Gemini AI
MAX_ITERATIONS=50
MAX_SUBORDINATE_AGENTS=5
MEMORY_LIMIT=1000
AGENT_TIMEOUT=300

# Web UI Configuration
WEB_UI_HOST=0.0.0.0
WEB_UI_PORT=8080
WEB_UI_DEBUG=false

# Storage Configuration
STORAGE_TYPE=json
STORAGE_PATH=./data
BACKUP_ENABLED=true
BACKUP_INTERVAL=3600

# Database Configuration (Legacy - not used with JSON storage)
DATABASE_URL=sqlite:///agent_zero.db
DATABASE_POOL_SIZE=10

# Search Configuration
SEARCH_ENGINE=duckduckgo
SEARCH_MAX_RESULTS=10
SEARCH_TIMEOUT=30

# Audio Configuration (Optional)
TTS_ENABLED=true
STT_ENABLED=true
AUDIO_DEVICE_INDEX=0
TTS_VOICE_INDEX=0
TTS_RATE=150
TTS_VOLUME=0.9

# Browser Configuration (Optional)
BROWSER_ENABLED=true
BROWSER_TYPE=chrome
HEADLESS_MODE=true
BROWSER_TIMEOUT=30

# Security Configuration
SECRET_KEY=your_secret_key_here
ALLOWED_HOSTS=localhost,127.0.0.1
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/agent_zero.log

# Tool Configuration
ENABLE_CODE_EXECUTION=true
ENABLE_TERMINAL_ACCESS=true
ENABLE_WEB_SEARCH=true
ENABLE_FILE_OPERATIONS=true
ENABLE_BROWSER_AUTOMATION=true
ENABLE_AUDIO_PROCESSING=true
ENABLE_DOCUMENT_PROCESSING=true

# Safety Configuration
SAFE_MODE=true
RESTRICTED_COMMANDS=rm,del,format,fdisk
ALLOWED_DIRECTORIES=./workspace,./tmp,./data
MAX_FILE_SIZE=100MB
MAX_EXECUTION_TIME=60
